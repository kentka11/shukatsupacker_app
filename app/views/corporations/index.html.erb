<p id="notice"><%= notice %></p>

<h1>Corporations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Category</th>
      <th>Profile</th>
      <th>Revenue</th>
      <th>Average income</th>
      <th>Es due date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @corporations.each do |corporation| %>
      <tr>
        <td><%= corporation.name %></td>
        <td><%= corporation.category %></td>
        <td><%= corporation.profile %></td>
        <td><%= corporation.revenue %></td>
        <td><%= corporation.average_income %></td>
        <td><%= corporation.es_due_date %></td>
        <td><%= link_to 'Show', corporation %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Corporation', new_corporation_path %>
